  object Fornecedor: TIBQuery
    Database = dmConnection.dbSig
    Transaction = dmConnection.TransSig
    SQL.Strings = (
      'select b."cod_fornecedor", b."raz_social_reduz"'
      'from "produto" a, "fornecedor" b'
      'where a."cod_empresa" = :cod_empresa'
      '  and a."cod_cliente" = :cod_cliente'
      '  and a."cod_fornecedor" = b."cod_fornecedor"'
      'group by 1,2'
      'order by 2,1')
    Left = 408
    Top = 50
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_empresa'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'cod_cliente'
        ParamType = ptUnknown
